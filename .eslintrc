{
 "parserOptions": {
   "ecmaVersion": 2018,
   "sourceType": "module"
 },
 "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "never",
      "exports": "never",
      "functions": "always-multiline"
    }],
   "semi": "off",
   "indent": ["error", 2],
   "quotes": ["error", "single"],
   "no-console": 1
 }
}
